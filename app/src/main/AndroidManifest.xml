<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.fanfatal.swipecontrollerdemo">

    <application
        android:name="com.first.mytwitter.Applycation.DefAppl"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.first.mytwitter.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.first.mytwitter.Main2Activity"
          >
            <intent-filter>
                <action android:name="com.first.anew.Main2Activity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.first.mytwitter.UpdateActivity"
            android:theme="@style/Theme.AppCompat.Dialog"/>
    </application>

</manifest>